package myPuzzle_TEST;

import static org.junit.jupiter.api.Assertions.*;

import org.junit.jupiter.api.Test;

class ppr_TESTTest {

	@Test
	void test() {
		
		int[][] arr= {{0, 1, 2, 3},
					   {1, 0, 2, 3},
					   {1, 2, 0, 3},
					   {1, 2, 3, 0}};
		
		ppr_TEST p = new ppr_TEST();
		
		for(int i=0; i < 4 ; i++) {
			int len = arr[i].length;
			assertEquals(i, p.getZeroPosition(arr[i], len));
		}
		
		
		
		
	}

}
